{
  "expo": {
    "name": "puneri-patya-mobile",
    "slug": "puneri-patya-mobile",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/buildericon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash3.png",
      "resizeMode": "cover",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "io.github.puneri-patya.puneri-patya-mobile",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location to let user select and view location of the pati."
      }
    },
    "android": {
      "package": "io.github.puneri_patya.puneri_patya_mobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/buildericon.png",
        "backgroundColor": "#fff"
      },
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "ACCESS_NETWORK_STATE",
        "FINE_LOCATION"
      ]
    }
  }
}
