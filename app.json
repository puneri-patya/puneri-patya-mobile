{
  "expo": {
    "name": "puneri-patya-mobile",
    "slug": "puneri-patya-mobile",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/buildericon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash3.png",
      "resizeMode": "cover",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload patya to the server.",
          "cameraPermission": "The app accesses your camera to let you click and upload patya to the server."
        }
      ]
    ]
  }
}